batch_name=$1;
num_workers=$2;
shift 2;
./coog redis -- celery qremove  $batch_name
./coog celery -- $num_workers $batch_name
./coog batch -v $data:/coog/data -- $batch_name $*
ret_value=$?
sleep 10
./coog celery-$batchname stop
./coog celery-$batchname logs 2>&1
./coog celery-$batchname rm
return ret_value
